{
  // coc-clangd
  "clangd.inlayHints.enable": true,
  "clangd.inlayHints.sep": " ",
  // coc-java(and java debug)
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.home": "/opt/environment/java/jdk-13.0.2",
  "java.jdt.ls.vmargs": "-javaagent:/home/waves/.m2/repository/org/projectlombok/lombok/1.18.10/lombok-1.18.10.jar",
  "java.configuration.maven.userSettings": "/home/waves/.m2/settings.xml",
  "java.import.maven.enabled": true,
  "yank.enableCompletion": false,
  "floaterm.completion.enable": false,
  "list.normalMappings": {
    "i": "do:previous"
  },
  // coc-prettier
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "markdown",
    "go",
    "json"
  ],
  "prettier.statusItemText": " ",
  // coc-go
  "go.checkForUpdates": "disabled",
  "go.goplsOptions": {
    "usePlaceholders": true,
    "analyses": {
      "nilfunc": true,
      "nilness": true,
      "unusedwrite": true
    }
  },
  // coc-git
  "git.showCommitInFloating": true,
  "git.enableGutters": true,
  "git.branchCharacter": "",
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": true,
  "git.virtualTextPrefix": " ❯❯❯ ",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.addedSign.text": "+",
  "git.changedSign.text": "➜",
  "git.removedSign.text": "-",
  "git.topRemovedSign.text": "▔",
  "git.changeRemovedSign.text": "-",
  "git.changedDecorator": "➜",
  "git.conflictedDecorator": "@",
  "git.stagedDecorator": "$",
  "git.untrackedDecorator": ">",
  "highlight.colors.enable": true,
  "highlight.document.enable": true,
  "highlight.colorNames.enable": false,
  "colors.filetypes": ["*"],
  "diagnostic.enableSign": true,
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "suggest.minTriggerInputLength": 1,
  "suggest.snippetIndicator": "  ",
  "coc.preferences.snippetStatusText": "  ",
  "suggest.disableMenuShortcut": false,
  "snippets.shortcut": "",
  "coc.source.file.shortcut": "",
  "coc.source.db.enable": true,
  "coc.source.db.shortcut": "",
  "coc.source.db.filetypes": ["sql", "mysql", "plsql"],
  "sql.database": "mariadb",
  "coc.source.around.shortcut": "ﮜ",
  "coc.source.buffer.shortcut": "",
  "coc.source.issues.shortcut": "",
  "coc.source.buffer.ignoreGitignore": true,
  "translator.engines": ["youdao"],
  "hover.floatConfig": { "border": true },
  "suggest.floatConfig": {
    "border": true,
    "highlight": "Normal",
    "borderhighlight": "LineNr"
  },
  "diagnostic.floatConfig": { "border": true },
  "signature.floatConfig": { "border": true },
  "suggest.completionItemKindLabels": {
    "keyword": " ",
    "variable": " ",
    "value": " ",
    "operator": " ",
    "function": " ",
    "reference": " ",
    "constant": " ",
    "method": " ",
    "struct": "פּ ",
    "class": " ",
    "interface": " ",
    "text": "ﭨ ",
    "enum": " ",
    "enumMember": " ",
    "module": " ",
    "color": " ",
    "property": " ",
    "field": " ",
    "unit": " ",
    "event": " ",
    "file": " ",
    "folder": " ",
    "snippet": " ",
    "typeParameter": " ",
    "default": " "
  },
  // coc-yaml
  "yaml.format.enable": true,
  "yaml.schemas": {
    "http://json.schemastore.org/swagger-2.0": ["*swagger.yaml", "*swagger.yml"]
  },
  // coc-json
  "json.schemaDownload.enable": true,
  "json.schemas": [
    {
      "fileMatch": ["/.vimspector.json"],
      "url": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json"
    }
  ],
  // coc-explorer
  "explorer.width": 30,
  "explorer.quitOnOpen": false,
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": false
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.file.column.indent.indentLine": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappingMode": "none",
  "explorer.openAction.strategy": "previousBuffer",
  "explorer.git.showIgnored": true,
  "explorer.keyMappings.global": {
    "<backspace>": "nomap",
    "k": "nodePrev",
    "j": "nodeNext",
    "<space>": "toggleSelection",
    "<tab>": "actionMenu",
    "h": "collapse",
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "l": ["expandable?", "expand", "open"],
    "<cr>": ["expandable?", "expand", "open"],
    // "h": "open:vsplit",
    "t": "open:tab",
    "N": "gotoParent",
    "u": "gotoParent",
    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "dd": "cutFile",
    "pp": "pasteFile",
    "DD": "deleteForever",
    "a": "addFile",
    "A": "addDirectory",
    "cw": "rename",
    ".": "toggleHidden",
    "zh": "toggleHidden",
    "R": "refresh",
    "?": "help",
    "q": "quit",
    "X": "systemExecute",
    "gd": "listDrive",
    "f": "search",
    "F": "searchRecursive",
    "B": "gotoSource:file",
    "b": "gotoSource:buffer",
    "[[": "sourcePrev",
    "]]": "sourceNext",
    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",
    "[c": "gitPrev",
    "]c": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  // coc-sumneko-lua
  "Lua.semantic.enable": false,
  "Lua.telemetry.enable": false,
  "Lua.runtime.version": "LuaJIT",
  "Lua.diagnostics.disable": ["lowercase-global"],
  "Lua.workspace.useGitIgnore": true,
  "Lua.diagnostics.globals": [
    "freeswitch",
    "session",
    "params",
    "XML_STRING",
    "XML_REQUEST",
    "argv"
  ],
  "Lua.hint.paramName": "All",
  "sumneko-lua.enableNvimLuaDev": true,
  "sumneko-lua.inlayHints.typeHintsPrefix": " ",
  "sumneko-lua.inlayHints.paramHintsPrefix": " ",
  // leetcode
  "leetcode.enabled": true,
  "leetcode.language": "golang"
}
